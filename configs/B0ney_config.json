{
  "folder_templates": [
    {
      "root_folder": "~/Downloads",
      "folders": [
        "Compressed",
        "Documents",
        "Documents/Word",
        "Documents/txt",
        "Documents/Pdf",
        "Documents/Ebook",
        "Documents/RSS",
        "Pictures",
        "Pictures/Vector",
        "Music",
        "Music/midi",
        "Music/Tracker",
        "Video",
        "Programs",
        "Programs/Java",
        "Programs/Shell",
        "Programs/Python",
        "Programs/Jupyter Notebook",
        "Programs/Android",
        "Programs/Sql",
        "Programs/Python Libraries",
        "Programs/Scratch",
        "Programs/DLL",
        "Folders",
        "Disk Images",
        "Torrents",
        "Torrents/Linux",
        "Torrents/Magnets",
        "Misc",
        "Misc/Valve stuff",
        "Misc/No extension",
        "Misc/Fonts",
        "Misc/Anki",
        "Misc/Bookmarks",
        "Misc/Excalidraw",
        "Misc/LMMS Projects",
        "Misc/LMMS Presets",
        "Misc/Krita Projects",
        "Misc/SoundFonts",
        "Misc/Paint NET Projects",
        "Unsorted"
      ],
      "place_for_unwanted": "~/Downloads/Folders"
    },
    {
      "root_folder": "~/Pictures",
      "folders": [
        "Screenshots",
        "Wallpapers"
      ],
      "place_for_unwanted": null
    }
  ],
  "operations": [
    {
      "scan_sources": [
        "~/Pictures",
        "~/Downloads/Pictures",
        "~/Downloads"
      ],
      "rules": [
        {
          "keywords": [
            "screenshot"
          ],
          "extensions": null,
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Pictures/Screenshots"
        },
        {
          "keywords": [
            "wallpaper",
            "unsplash",
            "pexels",
            "wallhaven",
            "1920x1080",
            "4k"
          ],
          "extensions": null,
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Pictures/Wallpapers"
        }
      ]
    },
    {
      "scan_sources": [
        "~/Downloads",
        "~/Downloads/Unsorted",
        "~/Downloads/Documents/"
      ],
      "rules": [
        {
          "keywords": null,
          "extensions": [
            "zip",
            "7z",
            "tar",
            "bz2",
            "rar",
            "xz",
            "gz",
            "zst"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Compressed"
        },
        {
          "keywords": null,
          "extensions": [
            "docx"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Documents/Word"
        },
        {
          "keywords": null,
          "extensions": [
            "pdf"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Documents/Pdf"
        },
        {
          "keywords": null,
          "extensions": [
            "epub"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Documents/Ebook"
        },
        {
          "keywords": null,
          "extensions": [
            "txt"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Documents/txt"
        },
        {
          "keywords": null,
          "extensions": [
            "rss",
            "atom"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Documents/RSS"
        },
        {
          "keywords": null,
          "extensions": [
            "jpeg",
            "jpg",
            "png",
            "bmp",
            "gif"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Pictures"
        },
        {
          "keywords": null,
          "extensions": [
            "svg"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Pictures/Vector"
        },
        {
          "keywords": null,
          "extensions": [
            "mp3",
            "mp2",
            "wav",
            "ogg",
            "aac",
            "flac",
            "alac",
            "dsd",
            "mqa",
            "m4a",
            "opus"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Music"
        },
        {
          "keywords": null,
          "extensions": [
            "it",
            "mod",
            "xm",
            "s3m",
            "umx",
            "mptm"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Music/Tracker"
        },
        {
          "keywords": null,
          "extensions": [
            "mid"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Music/midi"
        },
        {
          "keywords": null,
          "extensions": [
            "mp4",
            "mkv",
            "flv",
            "mov",
            "avi",
            "webm",
            "3gp"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Video"
        },
        {
          "keywords": null,
          "extensions": [
            "exe",
            "deb",
            "msi",
            "appimage",
            "AppImage",
            "msu",
            "appinstaller",
            "flatpak"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs"
        },
        {
          "keywords": null,
          "extensions": [
            "jar"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Java"
        },
        {
          "keywords": null,
          "extensions": [
            "py"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Python"
        },
        {
          "keywords": null,
          "extensions": [
            "sh"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Shell"
        },
        {
          "keywords": null,
          "extensions": [
            "ipynb"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Jupyter Notebook"
        },
        {
          "keywords": null,
          "extensions": [
            "apk"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Android"
        },
        {
          "keywords": null,
          "extensions": [
            "sql"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Sql"
        },
        {
          "keywords": null,
          "extensions": [
            "whl"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Python Libraries"
        },
        {
          "keywords": null,
          "extensions": [
            "dll"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/DLL"
        },
        {
          "keywords": null,
          "extensions": [
            "sb",
            "sb2",
            "sb3"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Programs/Scratch"
        },
        {
          "keywords": [
            "linux"
          ],
          "extensions": [
            "torrent"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Torrents/Linux"
        },
        {
          "keywords": null,
          "extensions": [
            "torrent"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Torrents"
        },
        {
          "keywords": null,
          "extensions": [
            "magnet"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Torrents/Magnets"
        },
        {
          "keywords": null,
          "extensions": [
            "iso",
            "img"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Disk Images"
        },
        {
          "keywords": null,
          "extensions": [
            "vtf",
            "vpk"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Valve stuff"
        },
        {
          "keywords": null,
          "extensions": [
            "apkg"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Anki"
        },
        {
          "keywords": [
            "bookmark"
          ],
          "extensions": [
            "html",
            "json"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Bookmarks"
        },
        {
          "keywords": null,
          "extensions": [
            "excalidraw",
            "drawio"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Excalidraw"
        },
        {
          "keywords": null,
          "extensions": [
            "ttf",
            "woff2",
            "woff"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Fonts"
        },
        {
          "keywords": null,
          "extensions": [
            "mmpz",
            "mmp"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/LMMS Projects"
        },
        {
          "keywords": null,
          "extensions": [
            "xpf",
            "xpt",
            "xptz"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/LMMS Presets"
        },
        {
          "keywords": null,
          "extensions": [
            "kra"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Krita Projects"
        },
        {
          "keywords": null,
          "extensions": [
            "pdn"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/Paint NET Projects"
        },
        {
          "keywords": null,
          "extensions": [
            "sf2",
            "sf3",
            "sfz"
          ],
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/SoundFonts"
        },
        {
          "keywords": null,
          "extensions": "",
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Misc/No extension"
        }
      ]
    },
    {
      "scan_sources": [
        "~/Downloads"
      ],
      "rules": [
        {
          "keywords": "",
          "extensions": null,
          "whitelist": null,
          "action": "MOVE",
          "destination": "~/Downloads/Unsorted/"
        }
      ]
    }
  ]
}